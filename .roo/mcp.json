{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/user/studio/.idx"
      ]
    },
    "sequentialthinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "firebase": {
      "command": "npx",
      "args": ["-y", "firebase-tools@14.15.2", "experimental:mcp"]
    },
    "genkit": {
      "command": "npm",
      "args": ["install", "@genkit-ai/mcp"]
    }
  }
}
